    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-router": "0.0.1",
    "mongoose": "^5.10.3",
    "mysql2": "^2.1.0",
    "nodemon": "^2.0.4",
    "pg": "^8.3.3",
    "pg-hstore": "^2.3.3",
    "prop-types": "^15.7.2",
    "sequelize": "^6.3.5"
